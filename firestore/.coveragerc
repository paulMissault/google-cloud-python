[run]
branch = True

[report]
omit =
    */firestore_v1beta1/proto/*_pb2.py
    */firestore_v1beta1/proto/*_pb2_grpc.py
    */firestore_v1beta1/gapic/*.py
fail_under = 100
show_missing = True
exclude_lines =
    # Re-enable the standard pragma
    pragma: NO COVER
    # Ignore debug-only repr
    def __repr__
